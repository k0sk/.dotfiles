URxvt*termName:     xterm-256color
URxvt*font: xft:    Source Code Pro:Medium:size=10:antialias=true,xft:Noto Sans CJK:Regular:size=9: antialiase=true
URxvt*cursorBlink:  true
URxvt*scrollBar:    false
URxvt.saveLines:    32767
URxvt*depth:        32

!!
!! Perl Extensions
!!
URxvt*perl-ext-common:   default,matche
URxvt*cutchars:          `"()'*<>[]{|}"
URxvt*matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
URxvt*matcher.button:    1
URxvt*url-launcher:      /usr/bin/chromium

!!
!! Colorscheme
!!
URxvt.foreground: #D7D0C7
URxvt.background: #151515
! Black
URxvt.color0:  #303030
URxvt.color8:  #404040
! Red
URxvt.color1:  #E84F4F
URxvt.color9:  #D23D3D
! Green
URxvt.color2:  #B8D68C
URxvt.color10: #A0CF5D
! Yellow
URxvt.color3:  #E1AA5D
URxvt.color11: #F39D21
! Blue
URxvt.color4:  #7DC1CF
URxvt.color12: #4E9FB1
! Magenta
URxvt.color5:  #9B64FB
URxvt.color13: #8542FF
! Cyan
URxvt.color6:  #6D878D
URxvt.color14: #42717B
! White
URxvt.color7:  #DDDDDD
URxvt.color15: #DDDDDD
