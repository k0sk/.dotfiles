URxvt*font: xft:Source Code Pro:Medium:size=10:antialias=true,xft:Noto Sans CJK:Regular:size=9:antialiase=true
URxvt*scrollBar: false
URxvt.saveLines: 32767
URxvt*termName:  rxvt
URxvt*depth:     32

!!
!! Perl Extensions
!!
URxvt*perl-ext-common: default,matcher
URxvt*url-launcher:    /usr/bin/chromium
URxvt*matcher.button:  1

